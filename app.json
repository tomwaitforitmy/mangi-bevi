{
  "expo": {
    "name": "mangi-bevi",
    "description": "An app for foodies to store your favorite recipes",
    "slug": "mangi-bevi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/noodles.png",
    "splash": {
      "image": "./assets/splash-noodles.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-noodles.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
